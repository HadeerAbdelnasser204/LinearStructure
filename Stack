#include <iostream>
#include <vector>
using namespace std;
template<class T>
class Stack {
private:
    vector<T> s;
    int size;
public:
    explicit Stack(int sz){
        size = sz;
    }
    void push(T element) {
        s.push_back(element);
    }

    T pop() {
        T ele = s.front();
        s.erase(s.begin());
        return ele;
    }

    T top() {
        return s.front();
    }

    bool isEmpty() {
        return size == 0;
    }

    int stackSize() {
        return size;
    }

    void clear() {
        size = 0;
    }
    void print(){
        for (int i = size - 1; i >= 0 ; --i) {
            cout << s[i] << " ";
        }
        cout << endl;
    }


};
